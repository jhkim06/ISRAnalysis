{
    "plotPrefix": "",
    "Analysis": "ISR",
    "Year": "2018",
    "Channel": "electron",
    "HistPrefix": "histo_",
    "topDirName": "Detector",
    "useTUnfoldBin": true,
    "TUnfoldBinNames": ["Rec_Pt","Rec_Mass"], 
    "Variables": ["Mass", "Pt_0", "Pt_1", "Pt_2", "Pt_3", "Pt_4"],
    "VariablePostfix": "FineCoarse",
    "Steering":
    {
        "Mass": "mass[];pt[C0O]",
        "Pt_0": "pt[];mass[UOC0]",
        "Pt_1": "pt[];mass[UOC1]",
        "Pt_2": "pt[];mass[UOC2]",
        "Pt_3": "pt[];mass[UOC3]",
        "Pt_4": "pt[];mass[UOC4]"
    },
    "Systematics": {
        "measurement": {
            "Nominal": ["Nominal"],
            "IdSF":  ["Up", "Down"],
            "trgSF": ["Up", "Down"],
            "PU":    ["Up", "Down"]
        },
        "theory": {
            "Scale": ["ABUp", "ABDown", "AUp", "ADown", "BUp", "BDown"]   
        }
    },
    "Samples":
    {
        "Data_Measured" :      ["EGamma"],
        "DY_Signal":         ["DYJetsToEE", "DYJets10to50ToEE"],
        "DYtautau_Background": ["DYJetsToTauTau", "DYJets10to50ToTauTau"],
        "VV_Background":       ["ZZ_pythia","WZ_pythia","WW_pythia"],
        "TTbar_Background":    ["TTLL_powheg"],
        "QCD_Background":      ["QCD"] 
    },
    "StackOrder": ["QCD_Background", "TTbar_Background", "VV_Background", "DYtautau_Background", "DY_Signal"]
}

